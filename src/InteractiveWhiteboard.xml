<?xml version="1.0" encoding="utf-8"?>
<widget id="auraq.interactivewhiteboard.InteractiveWhiteboard" pluginWidget="true" needsEntityContext="true" offlineCapable="true"
        supportedPlatform="Web"
        xmlns="http://www.mendix.com/widget/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.mendix.com/widget/1.0/ ../node_modules/mendix/custom_widget.xsd">
    <name>Interactive Whiteboard</name>
    <description>A whiteboard users can draw on and save images of creations</description>
    <icon>
	
	</icon>

	
    <properties>
        <propertyGroup caption="General">
			<propertyGroup caption="General">
				<property key="valueAttribute" type="attribute" required="true" onChange="onStatusChange">
					<caption>Whiteboard JSON</caption>
					<description>JSON attribute that stores whiteboard data for work to be saved and returned to.</description>
					<attributeTypes>
						<attributeType name="String"/>
					</attributeTypes>
				</property>
				<property key="onStatusChange" type="action">
					<caption>On change</caption>
                <description>Action to occur when whiteboard is saved (via save button or external action). If left to Do nothing, whiteboard is saved locally but not committed to the database.</description>
            </property>
			</propertyGroup>
			<propertyGroup caption="Editability">
					<property key="disablePages" type="boolean" defaultValue="false">
					<caption>Disable pages</caption>
					<description>Controls whether a whiteboard can have multiple pages</description>
				</property>
				<systemProperty key="Editability"/>
			</propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Display">
			<propertyGroup caption="Display">
				<property key="bootstrapStyle" type="enumeration" defaultValue="default">
					<caption>Whiteboard style</caption>
					<description />
					<enumerationValues>
						<enumerationValue key="default">Default</enumerationValue>
						<enumerationValue key="primary">Primary</enumerationValue>
						<enumerationValue key="success">Success</enumerationValue>
						<enumerationValue key="info">Info</enumerationValue>
						<enumerationValue key="inverse">Inverse</enumerationValue>
						<enumerationValue key="warning">Warning</enumerationValue>
						<enumerationValue key="danger">Danger</enumerationValue>
					</enumerationValues>
				</property>
				<property key="whiteboardBorder" type="enumeration" required="true" defaultValue="whiteboard_border_none">
					<caption>Include border</caption>
					<description>Adds border to whiteboard</description>
					<enumerationValues>
						<enumerationValue key="whiteboard_border_none">None</enumerationValue>
						<enumerationValue key="whiteboard_border_thin">Thin</enumerationValue>
						<enumerationValue key="whiteboard_border_medium">Medium</enumerationValue>
						<enumerationValue key="whiteboard_border_thick">Thick</enumerationValue>
					</enumerationValues>
				</property>
				<property key="whiteboardMode" type="enumeration" required="true" defaultValue="whiteboard_lightmode">
					<caption>Display mode</caption>
					<description>Renders whiteboard in light or dark mode</description>
					<enumerationValues>
						<enumerationValue key="whiteboard_lightmode">Light</enumerationValue>
						<enumerationValue key="whiteboard_darkmode">Dark</enumerationValue>
					</enumerationValues>
				</property>
				
			</propertyGroup>
			<propertyGroup caption="Dimensions">
                <property key="widthUnit" type="enumeration" defaultValue="auto">
                    <caption>Width unit</caption>
                    <description />
                    <enumerationValues>
                        <enumerationValue key="auto">Auto</enumerationValue>
                        <enumerationValue key="pixels">Pixels</enumerationValue>
                        <enumerationValue key="percentage">Percentage</enumerationValue>
                    </enumerationValues>
                </property>
				<property key="width" type="integer" defaultValue="200">
					<caption>Width</caption>
					<description />
                </property>
				<property key="heightUnit" type="enumeration" defaultValue="auto">
                    <caption>Height unit</caption>
                    <description />
                    <enumerationValues>
                        <enumerationValue key="auto">Auto</enumerationValue>
                        <enumerationValue key="pixels">Pixels</enumerationValue>
						<enumerationValue key="percentage">Percentage</enumerationValue>
                    </enumerationValues>
                </property>
				<property key="height" type="integer" defaultValue="200">
                    <caption>Height</caption>
                    <description />
                </property>
				
				<property key="allowResize" type="enumeration" defaultValue="allow_resize">
					<caption>Allow resize</caption>
					<description>Allows user to change the height and width of whiteboard dynamically</description>
					<enumerationValues>
                        <enumerationValue key="allow_resize">Yes</enumerationValue>
                        <enumerationValue key="no_resize">No</enumerationValue>
                    </enumerationValues>
				</property>
				
				<property key="restrictResize" type="enumeration" defaultValue="restrict_resize">
					<caption>Restrict resize</caption>
					<description>Limits the widget from being resized to the width and height of parent container</description>
					<enumerationValues>
                        <enumerationValue key="restrict_resize">Yes</enumerationValue>
                        <enumerationValue key="free_resize">No</enumerationValue>
                    </enumerationValues>
				</property>

			</propertyGroup>
        </propertyGroup>
        <propertyGroup caption="Events">
			<propertyGroup caption="Editor Actions">
				<property key="autoUpdate" type="enumeration" defaultValue="manual_update">
					<caption>Update whiteboard</caption>
					<description>Controls whether object containing whiteboard JSON is updated manually or automatically. Automatic update will cause whiteboard to refresh after inactivity.	
					</description>
					<enumerationValues>
                        <enumerationValue key="manual_update">Manual</enumerationValue>
                        <enumerationValue key="auto_update">Auto</enumerationValue>
                    </enumerationValues>
				</property>
			
			
				<property key="snapshotSave" type="boolean" defaultValue="true">
					<caption>Show update button</caption>
					<description>Adds button onto the whiteboard which users can click to update object holding JSON data</description>
				</property>
				<property key="snapshotLoad" type="boolean" defaultValue="true">
					<caption>Show reset button</caption>
					<description>Adds button onto the whiteboard which users can click to load their previously saved version of the whiteboard</description>
				</property>
				
				<property key="updateInterval" type="integer" defaultValue="1000">
					<caption>Update interval</caption>
					<description>Object will update and refresh whiteboard after this amount of time of inactivity has passed (100ms minimum)  </description>
                </property>
				
				<property key="keepTools" type="boolean" defaultValue="true">
					<caption>Persist toolbar</caption>
					<description>When whiteboard is refreshed, the current toolbox selection will stay</description>
                </property>
				
				
				
			</propertyGroup>
        </propertyGroup>
    </properties>
</widget>
